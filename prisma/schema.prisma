// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {

  id    Int     @id @default(autoincrement())

  email String  @unique

  name  String?
}

model WorkItem {

  id    String     @id @default(uuid())

  table String

  blocker Int

  stage Int

  owner String

  start Int

  end Int

  lead_time Int

  game_id String
}